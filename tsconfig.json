{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@utils/*": ["src/utils/*"],
      "@layouts/*": ["src/layouts/*"],
      "@components/*": ["src/components/*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
